{
  "name": "demand api",
  "test_type": {
    "Stress": {
      "times": 512
    }
  },
  "run_mode": "Parallel",
  "tests": [
    {
      "name": "empty request body & request header",
      "request": {
        "header": {
          "token": "",
          "element": "",
          "action": ""
        }
      },
      "expected_response": {
        "Err": {
          "kind": "MalformedRequest",
          "message": "request sanitization failure: invalid action detected: "
        }
      }
    },
    {
      "name": "empty request body",
      "request": {
        "header": {
          "element": "demand",
          "action": "create"
        }
      },
      "expected_response": {
        "Err": {
          "kind": "MalformedRequest",
          "message": "request sanitization failure: invalid action detected: "
        }
      }
    },
    {
      "name": "correct request",
      "request": {
        "header": {
          "element": "demand",
          "action": "create"
        },
        "creator": "simple",
        "organization": "simpleg",
        "end_product": {
          "value": 44321
        }
      },
      "expected_response": {
        "Ok": null
      }
    }
  ],
  "request_amqp_configuration": {
    "queue": {
      "name": "demand",
      "declare_options": {
        "passive": false,
        "durable": true,
        "exclusive": false,
        "auto_delete": true,
        "nowait": false
      },
      "declare_arguments": {}
    },
    "publish_options": {
      "mandatory": false,
      "immediate": false
    },
    "publish_arguments": {}
  },
  "reply_amqp_configuration": {
    "queue": {
      "name": "",
      "declare_options": {
        "passive": false,
        "durable": false,
        "exclusive": false,
        "auto_delete": false,
        "nowait": false
      },
      "declare_arguments": {}
    },
    "consume_options": {
      "no_local": false,
      "no_ack": false,
      "exclusive": false,
      "nowait": false
    },
    "consume_arguments": {}
  }
}